!--------------------xdefauls
URxvt.font: xft:monospace:pixelsize=12
urxvt*termName: rxvt
urxvt*scrollBar: false
urxvt*matcher.button: 1
URxvt*.transparent: true

URxvt.depth: 32
URxvt.background: rgba:1111/1111/1111/dddd
urxvt.boldFont:
Xft*dpi: 96
Xft*antialias: true
Xft*hinting: true
Xft*hintstyle: hintfull
Xft*rgba: rgb
URxvt*geometry: 85x20
*internalBorder: 7
URxvt*fading: 0
URxvt*tintColor: #ffffff
URxvt*.shading: 30
URxvt*inheritPixmap: False
! special
*.foreground:   #c5c8c6
*.background:   #333232
*.cursorColor:  #c5c8c6

! black
*.color0:       #585c69
*.color8:       #585c69

! red
*.color1:       #cab0a1
*.color9:       #cab0a1

! green
*.color2:       #9f8578
*.color10:      #9f8578

! yellow
*.color3:       #986c5b
*.color11:      #986c5b

! blue
*.color4:       #808178
*.color12:      #808178

! magenta
*.color5:       #979892
*.color13:      #979892

! cyan
*.color6:       #cab0a1
*.color14:      #cab0a1

! white
*.color7:       #707880
*.color15:      #707880

URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
URxvt.iso14755: false
URxvt.iso14755_52: false
URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.perl-ext-common: default,clipboard
